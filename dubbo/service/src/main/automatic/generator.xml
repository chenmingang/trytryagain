<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<plugin type="com.diy.generateGeneratePlugin" />

		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<!--<property name="searchString" value="Example$" />-->
			<!--<property name="replaceString" value="Criteria" />-->
		</plugin>

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/trytryagain" userId="root"
			password="Q12_test">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		
		<javaModelGenerator targetPackage="win.trytryagain.model"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="win.trytryagain.mapper"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="win.trytryagain.mapper"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

        <table tableName="promotion_ad" domainObjectName="PromotionAd">
            <property name="useActualColumnNames" value="true"/>
        </table>
	</context>
</generatorConfiguration>